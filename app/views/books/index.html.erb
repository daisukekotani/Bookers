<h1>Books</h1>
<% @books.each do |book| %>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><span><%= book.title %></span></td>
        <td><span><%= book.body %></span></td>
        <td><%= link_to "Show",book_path(book)%></td>
        <td><%= link_to "Edit",edit_book_path(book)%></td>
        <td><a href="Destroy">Destroy</a></td>
      </tr>
  </tbody>
</table>
<% end %>
<h2>New book</h2>
<%= form_with model: @book, url:'/books', local:true do |f| %>
<h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
  <% end %>
</form>

